@{
  Layout = "_Layout";
}
@model IEnumerable<ToDoList.Models.Item>

<div class='container'>
    <h2>To Do List</h2>
    <h4>@Html.DisplayNameForModel(model => model.Description)</h4>
    <ul>
        @foreach(var listItem in Model)
        {
            <li>@Html.DisplayFor(modelItem => listItem.Description)</li>
        }
    </ul>
</div>